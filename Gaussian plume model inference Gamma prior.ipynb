   "outputs": [],
   "source": [
    "# Gaussian Plume Model for concentration\n",
    "def C_func(x,y,z,u,a,b,Q,H):\n",
    "    C = Q/(2*a*x**b*np.pi*u)*(np.exp(-(y**2)/(2*a*x**b)))*(np.exp(-(z-H)**2/(2*a*x**b))+np.exp(-(z+H)**2/(2*a*x**b)))\n",
    "    return C"
   ]
  },
  {
